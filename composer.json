{
  "name": "aiir/cqured",
  "description": "PHP7+ WebAPI Framework",
  "version": "2.0.0-alpha1",
  "type": "project",
  "keywords": [
    "framework",
    "api",
    "database",
    "cqured"
  ],
  "homepage": "https://github.com/air-Design/cqured",
  "readme": "https://github.com/air-Design/cqured/blob/master/README.md",
  "time": "2018/10/14",
  "license": "MIT",
  "require": {
    "php": ">=7.2.0",
    "doctrine/orm": "^2.6.2",
    "guzzlehttp/psr7": "^1.4",
    "lcobucci/jwt": "^4.0.0-alpha1",
    "psr/http-message": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "symfony/yaml": "2.*"
  },
  "authors": [
    {
      "name": "Kofi Owusu-Afriyie",
      "email": "airdesign.code@gmail.com",
      "homepage": "https://github.com/air-Design",
      "role": "Developer"
    }
  ],
  "suggest": {
    "lcobucci/jwt": "Generate Json Web Tokens for Authentication"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Cqured\\Core\\": "vendor/cqured/cqured/core/",
      "Cqured\\Router\\": "vendor/cqured/cqured/router/",
      "Cqured\\Entity\\": "vendor/cqured/cqured/entity/",
      "Cqured\\MediatR\\": "vendor/cqured/mediatr/src/",
      "Presentation\\": "src/presentation/",
      "Common\\": "src/common/",
      "Core\\": "src/core/",
      "Infrastructure\\": "src/infrastructure/"
    }
  }
}
