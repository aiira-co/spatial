{
  "name": "spatial/spatial",
  "description": "PHP8+ WebAPI Clean Architecture Framework.",
  "version": "4.0.0-alpha4",
  "type": "project",
  "keywords": [
    "PHP",
    "Spatial",
    "Swoole",
    "Async",
    "API",
    "Docker",
    "Clean-Architecture"
  ],
  "homepage": "https://github.com/aiira_co/spatial",
  "readme": "https://github.com/aiira_co/spatial/blob/master/README.md",
  "time": "2023/11/11",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "ext-sodium": "*",
    "ext-openswoole": "*",
    "azjezz/psl": "^3.1.0",
    "lcobucci/jwt": "^5",
    "openswoole/core": "^22",
    "openswoole/ide-helper": "^22",
    "spatial/core": "^4.0.0-alpha3",
    "spatial/doctrine": "^4.0-alpha4",
    "symfony/yaml": "^6"
  },
  "authors": [
    {
      "name": "Kofi Owusu-Afriyie",
      "email": "koathecedi@gmail.com",
      "homepage": "https://aiira.co",
      "role": "Developer"
    }
  ],
  "suggest": {
    "lcobucci/jwt": "Generate Json Web Tokens for Authentication"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Presentation\\": "src/presentation/",
      "Common\\": "src/common/",
      "Core\\": "src/core/",
      "Infrastructure\\": "src/infrastructure/"
    }
  },
  "require-dev": {
    "pestphp/pest": "^3.7",
    "roave/security-advisories": "dev-master"
  },
  "scripts": {
    "ci": [
      "@phpcs",
      "@test"
    ],
    "phpstan": "phpstan analyse src --level=0",
    "phpcs": "phpcs --standard=PSR12 src",
    "test": "phpunit",
    "phpcbf": "phpcbf --standard=PSR12 src"
  }
}
